    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/manager/main.go",
            "env": { 
                "OPERATOR_NAME":"pipeline-operator", 
                "WATCH_NAMESPACE":"algorun", 
                "KUBERNETES_CONFIG": "$HOME/.kube/config",
                "NOTIF_URL":"http://localhost:5050/api/v1/notify", 
                "ALGORUNNER_IMAGE":"algohub/algo-runner:763cc4b9062371e33805452b79b64999f8de126f"
            }, 
            "args": [],
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 1024,
                "maxArrayValues": 1024,
                "maxStructFields": -1
            }
        }
    ]
}